{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended", "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {   
     "react/jsx-uses-react": "error",   
     "react/jsx-uses-vars": "error" ,"no-unused-vars": [
       "warn",
        {
            "varsIgnorePattern": "React"
        }
        
    ],        "no-warning-comments": "off"

}
}